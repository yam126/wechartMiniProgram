@model IEnumerable<DBModel.GoodSort>
@{
    ViewBag.Title = "ProductSortList";
}

<h2>ProductSortList</h2>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
       

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>            

            <td>
                @Html.ActionLink("Edit", "ProductEdit", new { id = item.GoodSortID }) |
                @Html.ActionLink("Details", "Details", new { id = item.GoodSortID }) |
                @Html.ActionLink("Delete", "ProductDelete", new { id = item.GoodSortID })
            </td>
        </tr>
    }

</table>