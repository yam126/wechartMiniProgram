            //urlBuf = url.toLowerCase();
            urlBuf = url;

            var youtubeVID = urlBuf.match(/(?:https?:\/{2})?(?:w{3}\.)?youtu(?:be)?\.(?:com|be)(?:\/watch\?v=|\/)([^\s&]+)/);
            if (youtubeVID != null) {
                return youtubeVID[1];
            } else {
                return "ERR";
            }

        }

